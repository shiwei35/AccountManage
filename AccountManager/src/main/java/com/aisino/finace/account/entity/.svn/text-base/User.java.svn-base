package com.aisino.finace.account.entity;

import java.util.List;

public class User extends MlUser {
	private MlRole currRole; // 当前role
	private MlRole defaultRole; // 系统默认role
	private MlOrg org; // 所属机构
	private String lastLoginDateStr;

	private List<MlRole> roles;
	private List<MlSysResc> rescs;
	private List<MlSysAuthority> authorities;

	public MlRole getCurrRole() {
		return currRole;
	}

	public void setCurrRole(MlRole currRole) {
		this.currRole = currRole;
	}

	public MlRole getDefaultRole() {
		return defaultRole;
	}

	public void setDefaultRole(MlRole defaultRole) {
		this.defaultRole = defaultRole;
	}

	public MlOrg getOrg() {
		return org;
	}

	public void setOrg(MlOrg org) {
		this.org = org;
	}

	public List<MlRole> getRoles() {
		return roles;
	}

	public void setRoles(List<MlRole> roles) {
		this.roles = roles;
	}

	public List<MlSysResc> getRescs() {
		return rescs;
	}

	public void setRescs(List<MlSysResc> rescs) {
		this.rescs = rescs;
	}

	public List<MlSysAuthority> getAuthorities() {
		return authorities;
	}

	public void setAuthorities(List<MlSysAuthority> authorities) {
		this.authorities = authorities;
	}

	public String getLastLoginDateStr() {
		return lastLoginDateStr;
	}

	public void setLastLoginDateStr(String lastLoginDateStr) {
		this.lastLoginDateStr = lastLoginDateStr;
	}

	
}
